{
  "name": "@orion/orion-design-system",
  "version": "1.8.7",
  "description": "Orion Design System - Web Components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/orion-web-components/orion-web-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/United-Airlines-Org/orion-design-system.git"
  },
  "publishConfig": {
    "registry": "https://artifactorycloud.ual.com/artifactory/api/npm/l-npm/"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "npm run webfont && concurrently \"npm run scss-watch\" \"stencil build --dev --watch --serve\"",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "clean-all": "npm run clean-stencil && npm run clean-storybook",
    "clean-stencil": "rimraf dist/ loader/",
    "clean-storybook": "rimraf storybook-static/",
    "storybook-no-legacy": "npm run clean-all && npm run build && start-storybook -p 6006 --quiet",
    "storybook": "npm run clean-all && npm run build && export SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006 --quiet",
    "dev-storybook-no-legacy": "npm run clean-all && concurrently -n stencil,storybook-no-legacy \"stencil build --watch\" \"npm:storybook-no-legacy\" \"npm run scss-watch\"",
    "dev-storybook": "npm run clean-all && concurrently -n stencil,storybook \"stencil build --watch\" \"npm:storybook\" \"npm run scss-watch\"",
    "build-storybook-no-legacy": "npm run clean-all && npm run build && build-storybook",
    "build-storybook": "npm run clean-all && npm run build && export SET NODE_OPTIONS=--openssl-legacy-provider && build-storybook",
    "lint-js": "eslint ./src",
    "lint-fix-js": "eslint ./src --fix",
    "lint-css": "prettier --check \"src/**/*.{css,scss}\"",
    "lint-fix-css": "prettier --write \"src/**/*.{css,scss}\"",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-fix": "npm run lint-fix-css && npm run lint-fix-js",
    "release-build": "npm run webfont && concurrently \"npm run scss-compile\" \"npm run build\"",
    "release-publish": "npm publish --registry https://artifactorycloud.ual.com/artifactory/api/npm/l-npm/",  
    "release": "npm run release-build && npm run release-publish",    
    "scss-compile": "sass src/components:src/components",
    "scss-watch": "sass --watch src/components:src/components",
    "webfont-clean-svgs": "svgo -f ./assets/16px -o ./src/assets/icons/sm --config svgoconfig.js && svgo -f ./assets/24px -o ./src/assets/icons/md --config svgoconfig.js",
    "webfont-rename": "renamer --chain renamer-case --case lower src/assets/icons/*/* --force && renamer --find \"/(\\d)\\ /g\" --replace \"$1\"  src/assets/icons/*/* --force && renamer --find \"/\\ /g\" --replace \"_\" src/assets/icons/*/* --force",
    "webfont-generate": "fantasticon --config .fantasticonrcSM.js && fantasticon --config .fantasticonrcMD.js",
    "webfont": "npm run webfont-clean-svgs && npm run webfont-rename && npm run webfont-generate",
    "prettier-check-js": "prettier --check \"src/**/*.{js, tsx}\"",
    "prettier-fix-js": "prettier --write \"src/**/*.{js, tsx}\"",
    "husky-prettier-fix": "cd ../.. && npm run husky-eslint-fix",
    "deploy": "cd ../../ && bash release.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{css,scss}": "prettier --write"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@stencil/core": "^2.17.1",
    "@stencil/react-output-target": "^0.3.1",
    "@types/prismjs": "^1.26.0",
    "classnames": "^2.3.2",
    "concurrently": "^7.3.0",
    "fantasticon": "^1.2.3",
    "flatpickr": "^4.6.13",
    "prismjs": "^1.29.0",
    "renamer": "^4.0.0",
    "renamer-case": "^2.0.2",
    "rimraf": "^3.0.2",
    "svgo": "^3.0.2",
    "tablesort": "^5.3.0"
  },
  "devDependencies": {
    "@angular/cli": "^14.1.3",
    "@babel/core": "^7.18.10",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil-community/postcss": "^2.1.0",
    "@stencil/vue-output-target": "^0.6.2",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-viewport": "6.5.13",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/html": "^6.1.21",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.10",
    "babel-loader": "^8.2.5",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "prettier": "2.7.1",
    "puppeteer": "^13.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.55.0"
  },
  "license": "MIT",
  "resolutions": {
    "@types/react": "^16.14.3"
  },
  "gitHead": "407d4b0b44b82ca85a532577c447bd00380bd0a0",
  "engines": {
    "node": "16.17.0"
  }
}
